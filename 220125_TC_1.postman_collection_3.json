	"item": [
		{
			"name": "Get_Data",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"//1.) StatusCode: 200\r",
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"//2.) ResponseTime ----- 2000msec\r",
							"pm.test(\"Response time is less than 200ms\", function () {\r",
							"    pm.expect(pm.response.responseTime).to.be.below(2000);\r",
							"});\r",
							"//3.) Response is JSON   ---- contentType ---- application/json\r",
							"pm.test(\"Content-Type is present\", function () {\r",
							"    pm.response.to.have.header(\"Content-Type\");\r",
							"    var contentType = pm.response.headers.get(\"Content-Type\");\r",
							"    pm.expect(contentType).to.include(\"application/json\");\r",
							"});\r",
							"\r",
							"//4.) status field is not empty\r",
							"pm.test(\"Name should not empty\", function () {\r",
							"pm.expect(pm.response.json().name).to.not.empty\r",
							"})\r",
							"\r",
							"//5.) id is 7\r",
							"pm.test(\"ID is 7\", function () {\r",
							"pm.expect(pm.response.json().id).to.eql(\"7\")\r",
							"})"
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
